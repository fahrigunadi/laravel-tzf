{
    "name": "fahrigunadi/laravel-tzf",
    "description": "Laravel Timezone Finder",
    "keywords": [
        "Fahri Gunadi",
        "laravel",
        "laravel-tzf"
    ],
    "homepage": "https://github.com/fahrigunadi/laravel-tzf",
    "license": "MIT",
    "authors": [
        {
            "name": "Fahri Gunadi",
            "email": "fahri@fahrigunadi.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^2.0||^3.0",
        "pestphp/pest-plugin-arch": "^2.5||^3.0",
        "pestphp/pest-plugin-laravel": "^2.0||^3.0"
    },
    "autoload": {
        "psr-4": {
            "FahriGunadi\\TimezoneFinder\\": "src/",
            "FahriGunadi\\TimezoneFinder\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FahriGunadi\\TimezoneFinder\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@composer run prepare",
            "@composer run publish-binary"
        ],
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "publish-binary": "FahriGunadi\\TimezoneFinder\\ComposerScripts::downloadBinary"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FahriGunadi\\TimezoneFinder\\TimezoneFinderServiceProvider"
            ],
            "aliases": {
                "TimezoneFinder": "FahriGunadi\\TimezoneFinder\\Facades\\TimezoneFinder"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}